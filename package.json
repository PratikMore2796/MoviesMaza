        "resolve-url-loader": "^3.1.0",
        "sass": "^1.43.4",
        "sass-loader": "^12.1.0",
        "vue-template-compiler": "^2.6.11",
        "webpack-cli": "^4.9.1"
    },
    "dependencies": {
        "@mui/icons-material": "^5.2.0",
        "material-icons": "^1.10.3",
        "tailwindcss": "^2.2.19",
        "tailwindcss-spinner": "^1.2.0",
    }
